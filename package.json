{
  "name": "ascii",
  "version": "0.1.0",
  "scripts": {
    "build:prod": "bsb -make-world && webpack --mode production",
    "build:dev": "bsb -make-world -w -ws _ && bsb -clean-world",
    "clean": "bsb -clean-world",
    "start:dev": "npm-run-all --parallel build:dev webpack:dev",
    "webpack:dev": "webpack-dev-server",
    "start:prod": "npm-run-all --parallel server build:prod",
    "clean:all": "rm -rf ./build",
    "server": "moduleserve ./ --port 8000"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0"
  },
  "devDependencies": {
    "bs-platform": "^6.2.1",
    "html-webpack-plugin": "^4.2.0",
    "moduleserve": "^0.8.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
